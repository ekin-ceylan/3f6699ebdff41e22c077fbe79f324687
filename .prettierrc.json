{
  "printWidth": 180,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "arrowParens": "avoid",
  "pugAttributeSeparator": "none",
  "pugClassLocation": "after-attributes",
  "pugSingleQuote": false,
  "pugSortAttributesBeginning": ["^:?id$", "^:?name$", "^:?label", "v-model", "value$"],
  "pugSortAttributesEnd": ["^:?mask", "validate", "^data-vv", "class", "type"],
  "pugPrintWidth": 500,
  "pugWrapAttributesThreshold": 12,
  "pugFramework": "vue",
  "pugIdNotation": "as-is"
}
